---
nats_servers:
<% p("nats.machines").each do |address| %>
- host: <%= address %>
  port: <%= p("nats.port") %>
  username: <%= p("nats.user") %>
  password: <%= p("nats.password") %>
<% end %>

name: <%= "#{spec.job.name}_#{spec.index.to_s}" %> 
publish_interval_in_seconds: <%= p('publish_interval_in_seconds') %>
payload_size_in_bytes: <%= p('payload_size_in_bytes') %>

